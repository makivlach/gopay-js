{
  "name": "@makivlach/gopay-js",
  "version": "1.5.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "gopay",
    "gopay-api",
    "payment-gateway",
    "payments"
  ],
  "files": [
    "/dist",
    "/@types"
  ],
  "publishConfig": {
    "@bscrypto-team:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fronebdev/gopay-js.git"
  },
  "author": "Froneb s.r.o. <hello@froneb.com>",
  "license": "MIT",
  "description": "The only functional library for GoPay payment gateway.",
  "devDependencies": {
    "@types/node": "^14.14.33",
    "@types/tap": "^14.10.3",
    "axios": "^0.24.0",
    "prettier": "^2.2.1",
    "tap": "^14.11.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "global": "^4.4.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3",
    "winston": "^3.3.3"
  },
  "scripts": {
    "compile": "tsc && tsc-alias",
    "start:dev": "tsnd .",
    "test": "tap tests/",
    "format": "prettier --write .",
    "prepublish": "yarn format && yarn compile && yarn test",
    "release:setup": "yarn config set version-git-tag true && yarn config set version-tag-prefix release-v",
    "release:patch": "yarn release:setup && yarn version --patch",
    "release:minor": "yarn release:setup && yarn version --minor",
    "release:major": "yarn release:setup && yarn version --major",
    "release:pre:major": "yarn release:setup && yarn version --premajor --preid pre",
    "release:pre:minor": "yarn release:setup && yarn version --preminor --preid pre",
    "release:pre:patch": "yarn release:setup && yarn version --prepatch --preid pre",
    "release:pre:release": "yarn release:setup && yarn version --prerelease --preid pre"
  },
  "bugs": {
    "url": "https://github.com/fronebdev/gopay-js/issues"
  },
  "homepage": "https://github.com/fronebdev/gopay-js#readme"
}
